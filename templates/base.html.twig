								</ul>
							</li>
						</ul>
						<!-- End  Articulos -->
					</nav>

				</aside>

				<main id="mainBar" class="col-lg-9">
					{% for label, messages in app.flashes %}
						{% for message in messages %}
							<div class="alert alert-{{ label }}" role="alert">
								{{ message }}
									</div>
								{% endfor %}
							{% endfor %}
							{% block body %}{% endblock %}
						</main>
						{# <div class="col col-lg-2"></div> #}
					</div>
				</div>
				<script src="{{asset('/js/hideAside.js')}}"></script>
			</body>
		</html>
	</body>
</html>
