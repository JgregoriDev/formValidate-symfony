{% extends 'base.html.twig' %}

{% block title %}Familia{% endblock %}

{% block body %}
    <h1>Familia</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Codfamilia</th>
                <td>{{ familium.codfamilia }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ familium.nombre }}</td>
            </tr>
            <tr>
                <th>Margen</th>
                <td>{{ familium.margen }}</td>
            </tr>
            <tr>
                <th>Ivapercent</th>
                <td>{{ familium.ivapercent }}</td>
            </tr>
            <tr>
                <th>Esmanoobra</th>
                <td>{{ familium.esmanoobra ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Iniciocodean</th>
                <td>{{ familium.iniciocodean }}</td>
            </tr>
            <tr>
                <th>Re</th>
                <td>{{ familium.re }}</td>
            </tr>
            <tr>
                <th>Imagen</th>
                <td>{{ familium.imagen }}</td>
            </tr>
            <tr>
                <th>Esanimales</th>
                <td>{{ familium.esanimales ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_familia_index') }}">back to list</a>

    <a href="{{ path('app_familia_edit', {'codfamilia': familium.codfamilia}) }}">edit</a>

    {{ include('familia/_delete_form.html.twig') }}
{% endblock %}
